<window id="129">
	<defaultcontrol always="false">3000</defaultcontrol>
	<!-- <zorder>20</zorder> -->
	<coordinates>
		<system>1</system>
		<left>0</left>
		<top>0</top>
	</coordinates>
	<controls>
		<control type="image">
			<description>underlay image</description>
			<left>0</left>
			<top>0</top>
			<width>1280</width>
			<height>720</height>
			<texture>main-underlay.png</texture>
			<animation effect="fade" start="0" end="100" time="200">WindowOpen</animation>
			<animation effect="fade" start="100" end="0" time="200">WindowClose</animation>
		</control>
		<control type="group">
			<left>447</left>
			<top>236</top>
			<animation effect="fade" start="0" end="100" delay="200" time="500">WindowOpen</animation>
			<animation effect="fade" start="100" end="0" time="500">WindowClose</animation>
			<control type="image">
				<description>background image</description>
				<left>0</left>
				<top>0</top>
				<width>386</width>
				<height>248</height>
				<texture>main-background.png</texture>
				<aspectratio>stretch</aspectratio>
			</control>
			<control type="label">
				<description>heading label</description>
				<left>0</left>
				<top>35</top>
				<width>386</width>
				<height>0</height>
				<align>center</align>
				<aligny>center</aligny>
				<font>size_20</font>
				<label>Clean Program Thumbs</label>
				<textcolor>FF000000</textcolor>
			</control>
			<control type="grouplist">
				<description>custom grouplist for context buttons</description>
				<left>43</left>
				<top>84</top>
				<width>386</width>
				<height>290</height>
				<itemgap>0</itemgap>
				<control type="button" id="3000">
					<description>Action Button</description>
					<width>300</width>
					<height>42</height>
					<font>size_23</font>
					<align>center</align>
					<textcolor>F5FFFFFFF</textcolor>
					<focusedcolor>FFFFFFFF</focusedcolor>
					<label>Clean Thumbs</label>
					<texturefocus border="2">main-button-focus.png</texturefocus>
					<texturenofocus border="2">main-button-nofocus.png</texturenofocus>
				</control>
				<control type="button" id="3001">
					<description>Action Button</description>
					<width>300</width>
					<height>42</height>
					<font>size_23</font>
					<align>center</align>
					<textcolor>F5FFFFFFF</textcolor>
					<focusedcolor>FFFFFFFF</focusedcolor>
					<label>Generate Thumbs</label>
					<texturefocus border="2">main-button-focus.png</texturefocus>
					<texturenofocus border="2">main-button-nofocus.png</texturenofocus>
				</control>
				<control type="button" id="3002">
					<description>Action Button</description>
					<width>300</width>
					<height>42</height>
					<font>size_23</font>
					<align>center</align>
					<textcolor>F5FFFFFFF</textcolor>
					<focusedcolor>FFFFFFFF</focusedcolor>
					<label>Remove Thumbs</label>
					<texturefocus border="2">main-button-focus.png</texturefocus>
					<texturenofocus border="2">main-button-nofocus.png</texturenofocus>
				</control>
			</control>
		</control>
	</controls>
</window>