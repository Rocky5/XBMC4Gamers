<includes>
		<include name="Dual3DView">
				<control type="group">
						<visible>Control.IsVisible(64) + !Control.IsVisible(50) + !Window.IsVisible(134) + !Window.IsVisible(3000)</visible>
						<control type="group">
								<control type="fixedlist" id="64">
										<left>-295</left>
										<top>320</top>
										<width>1700</width>
										<height>462</height>
										<onleft>64</onleft>
										<onright>64</onright>
										<onup>9000</onup>
										<ondown>90</ondown>
										<viewtype label="view64">list</viewtype>
										<pagecontrol>90</pagecontrol>
										<scrolltime>230</scrolltime>
										<focusposition>2</focusposition>
										<movement>0</movement>
										<orientation>horizontal</orientation>
										<preloaditems>4</preloaditems>
										<itemlayout height="290" width="414">
												<control type="group">
														<animation effect="slide" reversable="true" start="0,0" end="0,-20" time="0" condition="!Skin.HasSetting(_resourcesenabled)">Conditional</animation>
														<visible>!Skin.HasSetting(_resourcesenabled)</visible>
														<include>fallback_view_itemlayout</include>
												</control>
												<control type="group">
														<visible>Skin.HasSetting(_resourcesenabled)</visible>
														<control type="image">
																<left>-33</left>
																<top>5</top>
																<width>280</width>
																<height>280</height>
																<aspectratio align="" aligny="bottom">stretch</aspectratio>
																<texture background="true">no_dual3d.png</texture>
														</control>
														<control type="largeimage">
																<left>-33</left>
																<top>5</top>
																<width>280</width>
																<height>280</height>
																<aspectratio align="" aligny="bottom">stretch</aspectratio>
																<texture background="true">$INFO[listitem.path,,_resources\artwork\dual3d.png]</texture>
														</control>
												</control>
										</itemlayout>
										<focusedlayout height="290" width="414">
												<control type="group">
														<animation effect="slide" reversable="true" start="0,0" end="0,-20" time="0" condition="!Skin.HasSetting(_resourcesenabled)">Conditional</animation>
														<visible>!Skin.HasSetting(_resourcesenabled)</visible>
														<include>fallback_view_focusedlayout</include>
												</control>
												<control type="group">
														<visible>Skin.HasSetting(_resourcesenabled)</visible>
														<control type="image">
																<left>-33</left>
																<top>5</top>
																<width>280</width>
																<height>280</height>
																<aspectratio align="" aligny="bottom">stretch</aspectratio>
																<texture background="true">no_dual3d.png</texture>
																<animation reversible="false" effect="zoom" start="-33,5,280,280" end="-84,-84,384,384" time="100">focus</animation>
																<animation reversible="false" effect="zoom" end="-33,5,280,280" start="-84,-84,384,384" time="100">unfocus</animation>
														</control>
														<control type="largeimage">
																<left>-33</left>
																<top>5</top>
																<width>280</width>
																<height>280</height>
																<aspectratio align="" aligny="bottom">stretch</aspectratio>
																<texture background="true">$INFO[listitem.path,,_resources\artwork\dual3d.png]</texture>
																<animation reversible="false" effect="zoom" start="-33,5,280,280" end="-84,-84,384,384" time="100">focus</animation>
																<animation reversible="false" effect="zoom" end="-33,5,280,280" start="-84,-84,384,384" time="100">unfocus</animation>
														</control>
												</control>
										</focusedlayout>
								</control>
								<control type="label">
										<left>0</left>
										<top>615</top>
										<width>1280</width>
										<height>36</height>
										<font>size_35</font>
										<textcolor>$VAR[var-labelcolour]</textcolor>
										<scroll>true</scroll>
										<align>center</align>
										<aligny>center</aligny>
										<label>[UPPERCASE]$INFO[ListItem.Label][/UPPERCASE]</label>
										<animation effect="fade" start="100" end="0" time="0" condition="Skin.HasSetting(hidefoldernames) + !Container(64).ListItem.IsFolder">Conditional</animation>
										<animation effect="fade" start="100" end="0" time="0" condition="Skin.HasSetting(hidegamenames) + Container(64).ListItem.IsFolder">Conditional</animation>
								</control>
								<control type="grouplist">
										<left>0</left>
										<top>645</top>
										<width>1280</width>
										<itemgap>5</itemgap>
										<align>center</align>
										<orientation>horizontal</orientation>
										<visible>!Control.HasFocus(92) + !ListItem.IsFolder</visible>
										<animation effect="fade" start="100" end="0" time="0" condition="Skin.HasSetting(hidefoldernames) + !Container(64).ListItem.IsFolder">Conditional</animation>
										<animation effect="fade" start="100" end="0" time="0" condition="Skin.HasSetting(hidegamenames) + Container(64).ListItem.IsFolder">Conditional</animation>
										<control type="label">
												<width min="0" max="640">auto</width>
												<height>30</height>
												<font>size_15</font>
												<textcolor>$VAR[var-labelcolour]</textcolor>
												<scroll>true</scroll>
												<aligny>center</aligny>
												<label>$LOCALIZE[31186]</label>
												<visible>StringCompare(ListItem.ProgramCount,0)</visible>
										</control>
										<control type="label">
												<width min="0" max="640">auto</width>
												<height>30</height>
												<font>size_15</font>
												<textcolor>$VAR[var-labelcolour]</textcolor>
												<scroll>true</scroll>
												<aligny>center</aligny>
												<label>$LOCALIZE[31187] $INFO[ListItem.ProgramCount,, $LOCALIZE[31188]]</label>
												<visible>StringCompare(ListItem.ProgramCount,1)</visible>
										</control>
										<control type="label">
												<width min="0" max="640">auto</width>
												<height>30</height>
												<font>size_15</font>
												<textcolor>$VAR[var-labelcolour]</textcolor>
												<scroll>true</scroll>
												<aligny>center</aligny>
												<label>$LOCALIZE[31187] $INFO[ListItem.ProgramCount,, $LOCALIZE[31189]]</label>
												<visible>IntegerGreaterThan(ListItem.ProgramCount,1)</visible>
										</control>
										<control type="label">
												<width min="0" max="640">auto</width>
												<height>30</height>
												<font>size_15</font>
												<textcolor>$VAR[var-labelcolour]</textcolor>
												<scroll>true</scroll>
												<aligny>center</aligny>
												<label>- Has Trainer</label>
												<visible>!IsEmpty(listitem.overlay)</visible>
										</control>
								</control>
								<control type="scrollbar" id="90">
										<onup>64</onup>
										<include condition="Control.IsVisible(64)">ViewScrollBarHor</include>
								</control>
						</control>
				</control>
		</include>
</includes>